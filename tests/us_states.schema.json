{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "patternProperties": {
    "^[A-Z]{2}$": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "area": {
          "type": "object",
          "properties": {
            "total": { "type": "number" },
            "land": { "type": "number" },
            "water": { "type": "number" }
          },
          "required": ["total", "land", "water"],
          "additionalProperties": false
        },
        "neighbours": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "code": { "type": "string" },
              "name": { "type": "string" }
            },
            "required": ["code", "name"],
            "additionalProperties": false
          }
        },
        "median_age": { "type": "number" },
        "population_by_age_and_sex": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "from": { "type": "number" },
              "to": { "type": ["number", "null"] },
              "male": { "type": "number" },
              "female": { "type": "number" }
            },
            "required": ["from", "to", "male", "female"],
            "additionalProperties": false
          }
        },
        "ancestry": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "value": { "type": "number" }
            },
            "required": ["type", "value"],
            "additionalProperties": false
          }
        },
        "no_internet": { "type": "number" },
        "languages": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "value": { "type": "number" }
            },
            "required": ["type", "value"],
            "additionalProperties": false
          }
        },
        "foreign_born": { "type": "number" },
        "residential_mobility": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "value": { "type": "number" }
            },
            "required": ["type", "value"],
            "additionalProperties": false
          }
        },
        "veterans": {
          "type": "object",
          "properties": {
            "total": { "type": "number" },
            "male": { "type": "number" },
            "female": { "type": "number" }
          },
          "required": ["total", "male", "female"],
          "additionalProperties": false
        },
        "income": {
          "type": "object",
          "properties": {
            "total": { "type": "number" },
            "by_family": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": { "type": "string" },
                  "value": { "type": "number" }
                },
                "required": ["type", "value"],
                "additionalProperties": false
              }
            }
          }
		},
        "poverty": {
          "type": "object",
          "properties": {
            "total": { "type": "number" },
            "by_age": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "from": { "type": "number" },
                  "to": { "type": ["number", "null"] },
                  "value": { "type": "number" }
                },
                "required": ["from", "to", "value"],
                "additionalProperties": false
              }
            }
          },
          "required": ["total", "by_age"],
          "additionalProperties": false
        },
        "education": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
