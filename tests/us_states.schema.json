            "required": ["year", "value"],
            "additionalProperties": false
          }
        },
        "disability_rate": { "type": "number" },
        "disability_types": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "value": { "type": "number" }
            },
            "required": ["type", "value"],
            "additionalProperties": false
          }
        },
        "birth_by_mother_age": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
