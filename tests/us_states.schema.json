            "type": "object",
            "properties": {
              "from": { "type": "number" },
              "to": { "type": ["number", "null"] },
              "value": { "type": "number" }
            },
            "required": ["from", "to", "value"],
            "additionalProperties": false
          }
        },
        "housing_units": {
          "type": "object",
          "properties": {
            "total": { "type": "number" },
            "occupied": { "type": "number" },
            "vacant": { "type": "number" }
          },
          "required": ["total", "occupied", "vacant"],
		  "additionalProperties": false
