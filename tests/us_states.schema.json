        "health_insurance_coverage": {},
        "average_family_size": { "type": "number" },
        "marital_status": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "male": { "type": "number" },
              "female": { "type": "number" }
            },
            "required": ["type", "male", "female"],
            "additionalProperties": false
          }
        },
		"capital": { "type": ["string", "null"] },
		"largest_city": { "type": ["string", "null"] },
        "subdivisions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": { "type": "string" },
              "amount": { "type": "number" }
            },
            "required": ["type", "amount"],
            "additionalProperties": false
          }
        }
      },
      "required": [
        "name", "area", "neighbours", "median_age", "population_by_age_and_sex", "ancestry",
        "no_internet", "languages", "foreign_born", "residential_mobility", "veterans", "income",
		"poverty", "education", "school_enrollment", "worker_classes", "average_commute",
        "commute_type", "employment_rate", "work_hours", "median_rent", "rent", "homeownership_rate",
        "housing_value", "housing_units", "occupied_units_by_type", "bedrooms", "vacancy_rate",
        "disability_rate", "disability_types", "birth_by_mother_age", "health_insurance_coverage",
        "average_family_size", "marital_status", "capital", "largest_city", "subdivisions"
      ],
      "additionalProperties": false
    },
    "additionalProperties": false
  },
  "additionalProperties": false
}
